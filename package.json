{
  "name": "landscape-deploy",
  "version": "0.0.5",
  "description": "A command-line tool for executing Landscape scripts.",
  "author": "Alazhar Shamshuddin (ashamshuddin@surrey.ca)",
  "license": "ISC",
  "private": false,
  "bin": "./bin/landscape-deploy.js",
  "bugs": {
    "url": "https://github.com/CityofSurrey/landscape-deploy/issues"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": ["feat", "fix", "docs", "style", "refactor", "perf", "test", "chore", "revert"],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "The commit subject does not match the allowed pattern!",
      "helpMessage": ""
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityofSurrey/landscape-deploy.git"
  },
  "scripts": {
    "compile": "rm -rf bin && babel landscape-deploy.js --out-dir bin/ && sed -i '1 i\\#!/usr/bin/env node' bin/landscape-deploy.js",
    "prepublish": "npm run compile",
    "publish2npm": "npm run compile && publish"
  },
  "dependencies": {
    "bunyan": "^1.8.0",
    "commander": "^2.9.0",
    "node-print": "0.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "ghooks": "^1.2.1",
    "publish": "^0.6.0",
    "validate-commit-msg": "^2.6.1"
  }
}
